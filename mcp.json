{
  "servers": {
    "Context7": {
      "type": "stdio",
      "command": "context7-mcp",
      "fallback": {
        "command": "npx",
        "args": ["-y", "@upstash/context7-mcp@latest"]
      }
    },
    "Exa Search": {
      "command": "smithery",
      "args": ["run", "exa", "--key", "$MCP_API_KEY"],
      "type": "stdio",
      "enabled": true,
      "fallback": {
        "command": "npx",
        "args": [
          "-y",
          "@smithery/cli@latest",
          "run",
          "exa",
          "--key",
          "$MCP_API_KEY"
        ]
      }
    },
    "sequentialthinking": {
      "type": "stdio",
      "command": "mcp-server-sequential-thinking",
      "enabled": true,
      "fallback": {
        "command": "npx",
        "args": [
          "-y",
          "@modelcontextprotocol/server-sequential-thinking@latest"
        ]
      }
    },
    "office-word-mcp-server": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "@jackxzxz/office-word-mcp-server",
        "--key",
        "$MCP_API_KEY"
      ],
      "enabled": true
    },
    "fetch_mcp": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "@smithery-ai/fetch",
        "--key",
        "$MCP_API_KEY",
        "--profile",
        "plain-school-3XtE8q"
      ],
      "enabled": true
    }
  },
  "inputs": []
}
