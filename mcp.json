{
  "servers": {
    "fetch-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "fetch-mcp",
        "--key",
        "$MCP_API_KEY"
      ],
      "type": "stdio",
      "enabled": true,
      "fallback": {
        "command": "npx",
        "args": [
          "-y",
          "@smithery/cli@latest",
          "run",
          "fetch-mcp",
          "--key",
          "$MCP_API_KEY"
        ]
      }
    },
    "Context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp@latest"],
      "type": "stdio",
      "fallback": {
        "command": "npx",
        "args": ["-y", "@upstash/context7-mcp@latest"]
      }
    },
    "Exa Search": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "exa",
        "--key",
        "$MCP_API_KEY"
      ],
      "type": "stdio",
      "enabled": true,
      "fallback": {
        "command": "npx",
        "args": [
          "-y",
          "@smithery/cli@latest",
          "run",
          "exa",
          "--key",
          "$MCP_API_KEY"
        ]
      }
    },
    "sequentialthinking": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking@latest"],
      "type": "stdio",
      "enabled": true,
      "fallback": {
        "command": "npx",
        "args": [
          "-y",
          "@modelcontextprotocol/server-sequential-thinking@latest"
        ]
      }
    }
  },
  "inputs": []
}
