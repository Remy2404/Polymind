{
  "servers": {
    "fetch-mcp": {
      "command": "smithery",
      "args": ["run", "fetch-mcp", "--key", "$MCP_API_KEY"],
      "type": "stdio",
      "enabled": true,
      "fallback": {
        "command": "npx",
        "args": [
          "-y",
          "@smithery/cli@latest",
          "run",
          "fetch-mcp",
          "--key",
          "$MCP_API_KEY"
        ]
      }
    },
    "Context7": {
      "type": "stdio",
      "command": "context7-mcp",
      "fallback": {
        "command": "npx",
        "args": ["-y", "@upstash/context7-mcp@latest"]
      }
    },
    "Exa Search": {
      "command": "smithery",
      "args": ["run", "exa", "--key", "$MCP_API_KEY"],
      "type": "stdio",
      "enabled": true,
      "fallback": {
        "command": "npx",
        "args": [
          "-y",
          "@smithery/cli@latest",
          "run",
          "exa",
          "--key",
          "$MCP_API_KEY"
        ]
      }
    },
    "sequentialthinking": {
      "type": "stdio",
      "command": "mcp-server-sequential-thinking",
      "enabled": true,
      "fallback": {
        "command": "npx",
        "args": [
          "-y",
          "@modelcontextprotocol/server-sequential-thinking@latest"
        ]
      }
    }
  },
  "inputs": []
}
