{
  "version": 2,
  "builds": [
    {
      "src": "src/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.py"
    }
  ],
  "functions": {
    "src/main.py": {
      "maxDuration": 300
    }
  },
  "crons": [
    {
      "path": "/health",
      "schedule": "*/10 * * * *"
    }
  ]
}